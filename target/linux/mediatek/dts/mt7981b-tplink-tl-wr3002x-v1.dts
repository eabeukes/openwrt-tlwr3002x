/dts-v1/;

#include "mt7981b.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "TP-LINK TL-WR3002X";
	compatible = "tplink,tl-wr3002x-v1", "mediatek,mt7981";

	aliases {
		serial0 = &uart0;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&pio 33 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		mode {
			label = "mode";
			gpios = <&pio 36 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_2>;
		};
	};

	leds {
		compatible = "gpio-leds";

		red {
			color = <LED_COLOR_ID_RED>;
			function = "status";
			gpios = <&pio 34 GPIO_ACTIVE_HIGH>;
		};

		blue {
			color = <LED_COLOR_ID_BLUE>;
			function = "status";
			gpios = <&pio 35 GPIO_ACTIVE_HIGH>;
		};
	};

	reg_3p3v: regulator-3p3v {
		compatible = "regulator-fixed";
		regulator-name = "fixed-3.3V";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		regulator-always-on;
	};

	reg_5v: regulator-5v {
		compatible = "regulator-fixed";
		regulator-name = "fixed-5V";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-boot-on;
		regulator-always-on;
	};
};

&eth {
	status = "okay";
};

&mmc0 {
	pinctrl-names = "default", "state_uhs";
	pinctrl-0 = <&mmc0_pins_default>;
	pinctrl-1 = <&mmc0_pins_uhs>;
	bus-width = <4>;
	max-frequency = <50000000>;
	cd-gpios = <&pio 10 GPIO_ACTIVE_LOW>;
	wp-gpios = <&pio 11 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&pcie {
	status = "okay";
};

&pcie_phy {
	status = "okay";
};

&pio {
	mmc0_pins_default: mmc0-pins {
		mux {
			function = "sd";
			groups = "mmc0_1";
		};

		conf-cmd-dat {
			pins = "MMC_DAT0", "MMC_DAT1", "MMC_DAT2", "MMC_DAT3",
			       "MMC_CMD";
			input-enable;
			bias-disable;
			drive-strength = <MTK_DRIVE_4mA>;
		};

		conf-clk {
			pins = "MMC_CK";
			drive-strength = <MTK_DRIVE_4mA>;
			bias-disable;
		};
	};

	mmc0_pins_uhs: mmc0-uhs-pins {
		mux {
			function = "sd";
			groups = "mmc0_1";
		};

		conf-cmd-dat {
			pins = "MMC_DAT0", "MMC_DAT1", "MMC_DAT2", "MMC_DAT3",
			       "MMC_CMD";
			input-enable;
			bias-disable;
			drive-strength = <MTK_DRIVE_8mA>;
		};

		conf-clk {
			pins = "MMC_CK";
			drive-strength = <MTK_DRIVE_8mA>;
			bias-disable;
		};
	};

	spi2_pins: spi2-pins {
		mux {
			function = "spi";
			groups = "spi2";
		};
	};
};

&spi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi2_pins>;
	cs-gpios = <&pio 12 GPIO_ACTIVE_LOW>;
	status = "okay";

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "bl2";
				reg = <0x0 0x20000>;
				read-only;
			};

			partition@20000 {
				label = "fip";
				reg = <0x20000 0x100000>;
				read-only;
			};

			partition@120000 {
				label = "u-boot-env";
				reg = <0x120000 0x20000>;
			};

			partition@140000 {
				label = "firmware";
				reg = <0x140000 0x1bc0000>;
			};

			partition@1d00000 {
				label = "default_mac";
				reg = <0x1d00000 0x20000>;
				read-only;

				nvmem-layout {
					compatible = "fixed-layout";
					#address-cells = <1>;
					#size-cells = <1>;

					macaddr_default_mac_0: macaddr@8 {
						reg = <0x8 0x6>;
					};
				};
			};

			partition@1d20000 {
				label = "radio";
				reg = <0x1d20000 0xe0000>;
				read-only;

				nvmem-layout {
					compatible = "fixed-layout";
					#address-cells = <1>;
					#size-cells = <1>;

					eeprom_radio_0: eeprom@0 {
						reg = <0x0 0x80000>;
					};
				};
			};
		};
	};
};

&uart0 {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&wmac {
	mtd-eeprom = <&eeprom_radio_0>;
	status = "okay";
};

&eth_gmac0 {
	nvmem-cells = <&macaddr_default_mac_0>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <1>;
};